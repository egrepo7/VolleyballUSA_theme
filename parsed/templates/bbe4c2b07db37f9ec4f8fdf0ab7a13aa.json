{"components/custom/testimonials":"<h3 class=\"page-heading h1\">{{lang 'custom.home.testimonials-carousel.page-heading'}}</h3>\n<div class=\"button-quote-wrapper\">\n    <button aria-label=\"{{lang 'carousel.arrowAriaLabel'}}\" class=\"js-brand-prev-arrow slick-prev slick-arrow\">brand-prev-arrow</button>\n    <span class=\"icon icon--quote\">\n        <svg><use xlink:href=\"#icon-quote\"></use></svg>\n    </span>\n    <button aria-label=\"{{lang 'carousel.arrowAriaLabel'}}\" class=\"js-brand-next-arrow slick-next slick-arrow\">brand-next-arrow</button>\n</div>\n\n<section class=\"testimonial-carousel\"\n    data-slick='{\n        \"mobileFirst\": true,\n        \"slidesToShow\": 1,\n        \"slidesToScroll\": 1,\n        \"lazyLoad\": \"anticipated\",\n        \"dots\": \"false\",\n        \"slide\": \".js-brand-slide\",\n        \"prevArrow\": \".js-brand-prev-arrow\",\n        \"nextArrow\": \".js-brand-next-arrow\",\n        \"responsive\": [\n            {\n                \"breakpoint\": 800,\n                \"settings\": {\n                    \"slidesToShow\": 1,\n                    \"slidesToScroll\": 1\n                }\n            },\n            {\n                \"breakpoint\": 550,\n                \"settings\": {\n                    \"slidesToShow\": 1,\n                    \"slidesToScroll\": 1\n                }\n            }\n        ]\n    }'\n    data-dots-labels='{\n        \"dotAriaLabel\": \"{{lang 'carousel.dotAriaLabel'}}\",\n        \"activeDotAriaLabel\": \"{{lang 'carousel.activeDotAriaLabel'}}\"\n    }'>\n\n    <!-- <button aria-label=\"{{lang 'carousel.arrowAriaLabel'}}\" class=\"js-brand-prev-arrow slick-prev slick-arrow\">brand-prev-arrow</button> -->\n\n    {{#for 1 (lang 'custom.home.testimonials-carousel.number-of-testimonials')}}\n    <div class=\"js-brand-slide\" aria-label=\"{{#if this.alt_text.length '!=' 0}}{{this.alt_text}}{{else}}{{lang 'common.carousel_slide_number' slide_number=(add @index 1)}}{{/if}}\">\n        <div class=\"brand-carousel-slide {{#if ../theme_settings.homepage_stretch_carousel_images}}stretch{{/if}} {{#if @first}}brand-carousel-slide--first{{/if}}\">\n            <p class=\"testimonial-carousel__testimonial-body\">{{lang (concat (concat 'custom.home.testimonials-carousel.testimonial-' $index)'.testimonial-body')}}</p>\n            <p class=\"testimonial-carousel__testimonial-author h6\">{{lang (concat (concat 'custom.home.testimonials-carousel.testimonial-' $index)'.author')}}</p>\n            <p class=\"testimonial-carousel__testimonial-author-description\">{{lang (concat (concat 'custom.home.testimonials-carousel.testimonial-' $index)'.author-description')}}</p>\n        </div>\n    </div>\n    {{/for}}\n\n    <!-- <button aria-label=\"{{lang 'carousel.arrowAriaLabel'}}\" class=\"js-brand-next-arrow slick-next slick-arrow\">brand-next-arrow</button> -->\n\n</section>\n"}
