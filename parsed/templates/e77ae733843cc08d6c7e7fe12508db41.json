{"components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n","components/common/navigation-list":"{{#if children}}\n<a class=\"navPages-action has-subMenu {{#if hover_menu}}navPages-action--toplevel{{/if}}{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   data-collapsible=\"navPages-{{id}}\"\n   {{#if hover_menu}}data-collapsible-disabled-breakpoint=\"medium\"{{/if}}\n>\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\">\n            <a class=\"navPage-subMenu-action navPages-action\"\n               href=\"{{url}}\"\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n            >\n                {{lang 'category.view_all.name' category=name}}\n            </a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                        <span class=\"collapsible-icon-wrapper\"\n                            data-collapsible=\"navPages-{{id}}\"\n                            data-collapsible-disabled-breakpoint=\"medium\"\n                            data-collapsible-disabled-state=\"open\"\n                            data-collapsible-enabled-state=\"closed\"\n                        >\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n                            </i>\n                        </span>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                               href=\"{{url}}\"\n                               aria-label=\"{{name}}\"\n                            >\n                                {{name}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n        {{#if (lang (concat (concat 'custom.header.mega-menu.category-'@index)'.view')) '===' 'true'}}\n            <li class=\"navPage-subMenu-item navPage-subMenu-item--mega-menu\">\n                {{> components/custom/mega-menu }}\n            </li>\n        {{/if}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n","components/common/navigation-list-alternate":"{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       data-collapsible=\"navPages-{{id}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"socialLinks-link\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                    <span class=\"icon icon--{{display_name}}\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\"></use></svg>\n                    </span>\n                    <span class=\"socialLinks-label h6\">\n                        {{display_name}}\n                    </span>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/custom/header-2/header-2_navigation-list-static":"{{#if (lang 'custom.header.static.navigation-list.shop-all.is-enabled' '===' 'true')}}\r\n<li class=\"navPages-item navPages-item__shop-all\">\r\n    <a class=\"navPages-action has-subMenu navPages-action--toplevel\"\r\n        data-collapsible=\"navigation-item__shop-all\"\r\n        data-collapsible-disabled-breakpoint=\"medium\">\r\n        <span>\r\n            {{lang 'custom.header.static.navigation-list.shop-all.name'}}\r\n        </span>\r\n        <span class=\"icon icon--chevron-down navPages-action-moreIcon\">\r\n            <svg>\r\n                <use xlink:href=\"#icon-chevron-down\"></use>\r\n            </svg>\r\n        </span>\r\n    </a>\r\n    <div class=\"navPage-subMenu\" id=\"navigation-item__shop-all\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <ul class=\"navPage-subMenu-list\">\r\n            {{#for 1 (lang 'custom.header.static.navigation-list.shop-all.number-of-items')}}\r\n            <li \r\n                class=\"navPage-subMenu-item {{#if (lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.has-sub-items')) '===' 'true'}}has-submenu{{/if}}\" \r\n                data-mega-menu-toggle=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\" \r\n                data-collapsible=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\"\r\n                data-collapsible-disabled-breakpoint=\"medium\" \r\n                tabindex=\"-1\" \r\n            >\r\n                {{#if (lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.has-sub-items')) '===' 'true'}}\r\n                <a class=\"navPage-subMenu-action navPages-action has-subMenu\"\r\n                    href=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.url')}}\">\r\n                    <span>\r\n                        {{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.name')}}\r\n                    </span>\r\n                    <span class=\"icon icon--chevron-down\">\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-chevron-down\"></use>\r\n                        </svg>\r\n                    </span>\r\n                </a>\r\n                <a class=\"megaMenu-toggle icon icon--chevron-down\" href=\"##\">\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-chevron-down\"></use>\r\n                    </svg>\r\n                </a>\r\n                {{else}}\r\n                <a class=\"navPage-subMenu-action navPages-action\"\r\n                    href=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.url')}}\">\r\n                    <span>\r\n                        {{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.name')}}\r\n                    </span>\r\n                </a>\r\n                {{/if}}\r\n            </li>\r\n            {{/for}}\r\n        </ul>\r\n        <ul class=\"navPage-megaMenu-list\" style=\"float: right;\">\r\n            {{#for 1 (lang 'custom.header.static.navigation-list.shop-all.number-of-items')}}\r\n                {{#if (lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.is-enabled')) '===' 'true'}}\r\n                <li class=\"navPage-megaMenu\" data-mega-menu-item=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\" id=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\">\r\n                    <span class=\"navPage-megaMenu__heading\">\r\n                        {{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\r\n                    </span>\r\n                    <div class=\"navPage-megaMenu__content\">\r\n                        {{#for 1 (lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.number-of-mega-menu-items'))}}\r\n                        <div class=\"navPage-megaMenu__column\">\r\n                            <a class=\"navPage-megaMenu__column-link\"\r\n                                href=\"{{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-' ../$index)'.mega-menu.mega-menu-item-') $index) \".url\")}}\">\r\n                                <img class=\"navPage-megaMenu__image\"\r\n                                    src=\"{{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-' ../$index)'.mega-menu.mega-menu-item-') $index) \".imageUrl\")}}\"\r\n                                    alt=\"{{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-' ../$index)'.mega-menu.mega-menu-item-') $index) \".imageAltText\")}}\">\r\n                                <span>\r\n                                    {{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../$index)'.mega-menu.mega-menu-item-') $index) \".name\")}}\r\n                                </span>\r\n                            </a>\r\n                            {{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../$index)'.mega-menu.mega-menu-item-') $index) \".name\")}}\r\n                            <ul>\r\n                                {{#for 1 (lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../$index)'.mega-menu.mega-menu-item-') $index) \".number-of-sub-items\"))}}\r\n                                    <li class=\"mega-menu-sub-item\">\r\n                                        <a href=\"{{lang (concat (concat (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../../$index)'.mega-menu.mega-menu-item-') ../$index) '.sub-item-') $index) '.url')}}\">{{lang (concat (concat (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../../$index)'.mega-menu.mega-menu-item-') ../$index) '.sub-item-') $index) '.name')}}</a>\r\n                                    </li>\r\n                                {{/for}}\r\n                            </ul>\r\n                        </div>\r\n                        {{/for}}\r\n                    </div>\r\n                </li>\r\n                {{/if}}\r\n            {{/for}}\r\n        </ul>\r\n    </div>\r\n</li>\r\n{{/if}}\r\n{{#for 1 (lang 'custom.header.static.navigation-list.navigation-items.number-of-items')}}\r\n<li class=\"navPages-item no-megaMenu\">\r\n    {{#if (lang (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' $index)'.has-sub-items')) '===' 'true'}}\r\n    <a class=\"navPages-action has-subMenu no-megaMenu\"\r\n        href=\"{{lang (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' $index)'.url')}}\" \r\n        data-collapsible=\"navigation-item__item-{{$index}}\"\r\n        data-collapsible-disabled-breakpoint=\"medium\">\r\n        <div class=\"navPages-action__wrapper\">\r\n            <span>\r\n                {{lang (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' $index)'.name')}}\r\n            </span>\r\n            <span class=\"icon icon--chevron-down navPages-action-moreIcon\">\r\n                <svg>\r\n                    <use xlink:href=\"#icon-chevron-down\"></use>\r\n                </svg>\r\n            </span>\r\n        </div>\r\n    </a>\r\n    <div class=\"navPage-subMenu\" id=\"navigation-item__item-{{$index}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <ul class=\"navPage-subMenu-list\">\r\n            {{#for 1 (lang (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' $index)'.number-of-sub-items'))}}\r\n            {{#if (lang (concat (concat (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' ../$index)'.sub-item-')$index)'.has-sub-items')) '===' 'true'}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                <a \r\n                    class=\"navPage-subMenu-action navPages-action no-megaMenu\"\r\n                    href=\"{{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' ../$index)'.sub-item-') $index) '.url')}}\"\r\n                >\r\n                    <div class=\"navPage-subMenu-action__wrapper\">\r\n                        <span>\r\n                            {{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' ../$index)'.sub-item-') $index) '.name')}}\r\n                        </span>\r\n                        <span class=\"collapsible-icon-wrapper\"\r\n                            data-collapsible=\"navPages__item-{{../$index}}--sub-item-{{$index}}\"\r\n                            data-collapsible-disabled-breakpoint=\"medium\"\r\n                            data-collapsible-disabled-state=\"open\"\r\n                            data-collapsible-enabled-state=\"closed\"\r\n                        >\r\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                            </i>\r\n                        </span>\r\n                    </div>\r\n                </a>\r\n                <ul class=\"navPage-childList\" id=\"navPages__item-{{../$index}}--sub-item-{{$index}}\">\r\n                    {{#for (lang (concat (concat (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' ../$index)'.sub-item-')$index)'.number-of-sub-items'))}}\r\n                    <li class=\"navPage-childList-item\">\r\n                        <a class=\"navPage-childList-action navPages-action no-megaMenu\"\r\n                            href=\"{{lang (concat (concat (concat (concat (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' ../../$index)'.sub-item-')../$index)'.sub-item-')$index)'.url')}}\"\r\n                            aria-label=\"{{lang (concat (concat (concat (concat (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' ../../$index)'.sub-item-')../$index)'.sub-item-')$index)'.name')}}\"\r\n                        >\r\n                            <div class=\"navPage-childList-action__wrapper\">\r\n                                {{lang (concat (concat (concat (concat (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' ../../$index)'.sub-item-')../$index)'.sub-item-')$index)'.name')}}\r\n                            </div>\r\n                        </a>\r\n                    </li>\r\n                    {{/for}}\r\n                </ul>\r\n            </li>\r\n            {{else}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                <a \r\n                    class=\"navPage-subMenu-action navPages-action no-megaMenu\"\r\n                    href=\"{{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' ../$index)'.sub-item-') $index) '.url')}}\"\r\n                >\r\n                    <div class=\"navPage-subMenu-action__wrapper\">\r\n                        <span>\r\n                            {{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' ../$index)'.sub-item-') $index) '.name')}}\r\n                        </span>\r\n                    </div>\r\n                </a>\r\n            </li>\r\n            {{/if}}\r\n            {{/for}}\r\n        </ul>\r\n    </div>\r\n    {{else}}\r\n    <a class=\"navPages-action has-subMenu no-megaMenu\" href=\"{{lang (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' $index)'.url')}}\">\r\n        <div class=\"navPages-action__wrapper\">\r\n            <span>\r\n                {{lang (concat (concat 'custom.header.static.navigation-list.navigation-items.item-' $index)'.name')}}\r\n            </span>\r\n        </div>\r\n    </a>\r\n    {{/if}}\r\n</li>\r\n{{/for}}\r\n","components/custom/header-2/header-2_navigation-menu":"<nav class=\"navPages\">\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}} links\">\n\n        {{#if theme_settings.navigation_design '===' 'static'}}\n            {{> components/custom/header-2/header-2_navigation-list-static hover_menu=theme_settings.its.toggle_hover_menu}}\n        {{else}}\n            {{#each categories}}\n            <li class=\"navPages-item\">\n                {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                    {{> components/common/navigation-list hover_menu=../theme_settings.its.toggle_hover_menu}}\n                {{/if}}\n                {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n                    {{> components/common/navigation-list-alternate}}\n                {{/if}}\n                </li>\n            {{/each}}\n        {{/if}}\n        {{#all \n            (unless theme_settings.hide_content_navigation) \n            (unless theme_settings.navigation_design '===' 'static')\n        }}\n            {{#each pages}}\n                 <li class=\"navPages-item navPages-item-page\">\n                     <a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\"\n                        href=\"{{url}}\"\n                        aria-label=\"{{name}}\"\n                     >\n                         {{name}}\n                     </a>\n                 </li>\n             {{/each}}\n        {{/all}}\n    </ul>\n    <ul class=\"navPages-list header-2__navPages-list--custom\">\n        {{#for 1 (lang 'custom.header.top-navigation.number-of-links')}}\n            <li class=\"navPages-item\">\n                <a href=\"{{lang (concat (concat 'custom.header.top-navigation.link-'$index)'.link')}}\" class=\"navPages-action\">\n                    {{lang (concat (concat 'custom.header.top-navigation.link-'$index)'.text')}}\n                </a>\n            </li>\n        {{/for}}\n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\"\n                   href=\"#\"\n                   data-collapsible=\"navPages-currency\"\n                   aria-controls=\"navPages-currency\"\n                   aria-expanded=\"false\"\n                   aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\n                >\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}}\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use>\n                        </svg>\n                    </i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{{switch_url}}}\"\n                               aria-label=\"{{name}}\"\n                               data-currency-code=\"{{{code}}}\"\n                               {{#if cart_currency_switch_url.length '>' 0 }}\n                               data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                               {{/if}}\n                               data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                            >\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    {{lang 'common.gift_cert'}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\"\n                   href=\"{{urls.compare}}\"\n                   data-compare-nav\n                   aria-label=\"{{lang 'common.compare'}}\"\n                >\n                    {{lang 'common.compare'}}\n                    <span class=\"countPill countPill--positive countPill--alt\"></span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\"\n                   href=\"{{urls.account.index}}\"\n                   data-collapsible=\"navPages-account\"\n                   aria-label=\"{{lang 'common.account'}}\"\n                >\n                    {{lang 'common.account'}}\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n                    </i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.orders.all}}\"\n                               aria-label=\"{{lang 'account.nav.orders'}}\"\n                            >\n                                {{lang 'account.nav.orders'}}\n                            </a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.returns}}\"\n                                   aria-label=\"{{lang 'account.nav.returns'}}\"\n                                >\n                                    {{lang 'account.nav.returns'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.inbox}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.messages'}}\"\n                            >\n                                {{lang 'account.mobile_nav.messages'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.addresses}}\"\n                               aria-label=\"{{lang 'account.nav.addresses'}}\"\n                            >\n                                {{lang 'account.nav.addresses'}}\n                            </a>\n                        </li>\n                        {{#if settings.show_payment_methods}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.payment_methods.all}}\"\n                                   aria-label=\"{{lang 'account.nav.payment_methods'}}\"\n                                >\n                                    {{lang 'account.nav.payment_methods'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.wishlists.all}}\"\n                               aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\"\n                            >\n                                {{lang 'account.mobile_nav.wishlists'}}\n                            </a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.recent_items}}\"\n                               aria-label=\"{{lang 'account.nav.recently_viewed