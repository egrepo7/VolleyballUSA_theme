{"components/custom/home-callouts":"<h3 class=\"home-callouts__heading page-heading h1\">{{lang 'custom.home.order-callouts.heading'}}</h3>\n<section class=\"home-callouts\">\n        {{#for 1 (lang 'custom.home.order-callouts.number-of-callouts')}}\n        <div class=\"home-callouts__callout\">\n            <figure class=\"home-callouts__image-container\">\n                <img class=\"home-callouts__image lazyload\" data-src=\"{{lang (concat (concat 'custom.home.order-callouts.callout-'$index)'.image')}}\" src=\"{{cdn 'img/loading.svg'}}\" alt=\"{{lang (concat (concat 'custom.home.order-callouts.callout-'$index)'.image-alt-text')}}\">\n            </figure>\n            <article class=\"home-callouts__content\">\n                <h4 class=\"home-callouts__card-title h2\">{{lang (concat (concat 'custom.home.order-callouts.callout-'$index)'.title')}}</h4>\n                <p class=\"home-callouts__description\">{{lang (concat (concat 'custom.home.order-callouts.callout-'$index)'.description')}}</p>\n            </article>\n        </div>\n        {{/for}}\n</section>\n"}
