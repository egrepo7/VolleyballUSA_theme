{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n           href=\"#\"\n           data-dropdown=\"currencySelection\"\n           aria-controls=\"currencySelection\"\n           aria-expanded=\"false\"\n        >\n            {{lang 'common.currency' code=currency_selector.active_currency_code}}\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use xlink:href=\"#icon-chevron-down\" />\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n","components/common/navigation-list":"{{#if children}}\n<a class=\"navPages-action has-subMenu {{#if hover_menu}}navPages-action--toplevel{{/if}}{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   data-collapsible=\"navPages-{{id}}\"\n   {{#if hover_menu}}data-collapsible-disabled-breakpoint=\"medium\"{{/if}}\n>\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\">\n            <a class=\"navPage-subMenu-action navPages-action\"\n               href=\"{{url}}\"\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n            >\n                {{lang 'category.view_all.name' category=name}}\n            </a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                        <span class=\"collapsible-icon-wrapper\"\n                            data-collapsible=\"navPages-{{id}}\"\n                            data-collapsible-disabled-breakpoint=\"medium\"\n                            data-collapsible-disabled-state=\"open\"\n                            data-collapsible-enabled-state=\"closed\"\n                        >\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n                            </i>\n                        </span>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                               href=\"{{url}}\"\n                               aria-label=\"{{name}}\"\n                            >\n                                {{name}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n        {{#if (lang (concat (concat 'custom.header.mega-menu.category-'@index)'.view')) '===' 'true'}}\n            <li class=\"navPage-subMenu-item navPage-subMenu-item--mega-menu\">\n                {{> components/custom/mega-menu }}\n            </li>\n        {{/if}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n","components/common/navigation-list-alternate":"{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       data-collapsible=\"navPages-{{id}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}\n","components/common/quick-search":"<div class=\"container\">\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\"\n                       data-search-quick\n                       name=\"{{name}}\"\n                       id=\"{{name}}\"\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n                       autocomplete=\"off\"\n                >\n                <span class=\"icon icon--search\">\n                    <svg><use xlink:href=\"#icon-search\"></use></svg>\n                </span>\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n    <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p>\n</div>\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"socialLinks-link\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                    <span class=\"icon icon--{{display_name}}\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\"></use></svg>\n                    </span>\n                    <span class=\"socialLinks-label h6\">\n                        {{display_name}}\n                    </span>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/store-logo":"<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n        <img class=\"header-logo-image-unknown-size\"\n             src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n             alt=\"{{settings.store_logo.title}}\"\n             title=\"{{settings.store_logo.title}}\">\n        {{else}}\n        <div class=\"header-logo-image-container\">\n            <img class=\"header-logo-image\"\n                 src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                 srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n                 alt=\"{{settings.store_logo.title}}\"\n                 title=\"{{settings.store_logo.title}}\">\n        </div>\n        {{/if}}\n    {{else}}\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n","components/custom/header-2/header-2":"<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\n{{#if banners.top}}\n<div class=\"banners\" data-banner-location=\"top\">\n    {{#each (limit banners.top_metadata 1)}}\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\n        <div data-event-type=\"promotion-click\">\n            {{{this.content}}}\n        </div>\n    </div>\n    {{/each}}\n</div>\n{{/if}}\n\n\n<header class=\"header header-2 header-2--default\" role=\"banner\">\n    <div class=\"header-2__top\">\n        {{> components/custom/header-2/header-2_top-nav}}\n    </div>\n    \n    <div class=\"header-2__mid\">\n        <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n        </a>\n            {{!-- Static Header Top Navigation - Store Logo --}}\n            <div class=\"top-navigation__store-logo\">\n                {{#if page_type '===' 'default'}}\n                    <h1 class=\"header-logo\">\n                        {{> components/common/store-logo}}\n                    </h1>\n                {{else}}\n                    <div class=\"header-logo\">\n                        {{> components/common/store-logo}}\n                    </div>\n                {{/if}}\n            </div>\n            {{!-- Static Header Top Navigation - Store Contact --}}\n            <div class=\"top-navigation__store-contact\">\n                <a class=\"store-contact__link\">\n                    <span class=\"store-contact__label\">\n                        <span class=\"icon icon--phone\">\n                            <svg><use xlink:href=\"#icon-phone\"></use></svg>\n                        </span>\n                        <span>\n                            {{lang 'custom.header.static.top-navigation.store-contact.label'}}&nbsp;&nbsp;\n                        </span>\n                    </span>\n                    <a href=\"tel:{{settings.phone_number}}\" class=\"store-contact__phone-number\">\n                        {{settings.phone_number}}\n                    </a>\n                </a>\n            </div>\n        <div class=\"dropdown dropdown--quickSearch header-2__nav-search\" id=\"quickSearch\" aria-hidden=\"true\" data-prevent-quick-search-close>\n            {{> components/common/quick-search name='nav-quick-search'}}\n        </div>\n        {{> components/custom/header-2/header-2_navigation}}\n    </div>\n    \n    <div class=\"navPages-container\" id=\"menu\" data-menu>\n        {{> components/custom/header-2/header-2_navigation-menu}}\n    </div>\n</header>\n\n{{{region name=\"header_bottom--global\"}}}\n{{{region name=\"header_bottom\"}}}\n","components/custom/header-2/header-2_navigation":"<nav class=\"navUser\">\n    <ul class=\"navUser-section navUser-section--alt\">\n        <li class=\"navUser-item navUser-item--search\">\n            <button class=\"navUser-action navUser-action--quickSearch\" type=\"button\" id=\"quick-search-expand\"\n                data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-label=\"{{lang 'common.search'}}\">\n                <span class=\"icon icon--search\">\n                    <svg>\n                        <use xlink:href=\"#icon-search\"></use>\n                    </svg>\n                </span>\n                <span class=\"is-srOnly\">\n                    {{lang 'common.search'}}\n                </span>\n            </button>\n        </li>\n\n        <li class=\"navUser-item navUser-item--account\">\n            {{#if customer}}\n            <a class=\"navUser-action\" href=\"{{urls.account.index}}\" aria-label=\"{{lang 'common.account'}}\"\n                data-dropdown=\"account-dropdown\">\n                <span class=\"icon icon--account\">\n                    <svg>\n                        <use xlink:href=\"#icon-account\"></use>\n                    </svg>\n                </span>\n                <span class=\"is-srOnly\">\n                    {{lang 'common.account'}}\n                </span>\n                <span>{{lang 'common.account'}}</span>\n                <span class=\"icon icon--chevron-down\">\n                    <svg>\n                        <use xlink:href=\"#icon-chevron-down\"></use>\n                    </svg>\n                </span>\n            </a>\n\n            <div class=\"dropdown-menu header-2__mid-account-dropdown\" id=\"account-dropdown\" data-dropdown-content\n                aria-hidden=\"true\">\n                <ul class=\"header-2__mid-account-list\">\n                    <li class=\"header-2__mid-account-list-item\">\n                        <a class=\"header-2__mid-account-list-item-link\" href=\"{{urls.account.orders.all}}\"\n                            aria-label=\"{{lang 'account.nav.orders'}}\">\n                            {{lang 'account.nav.orders'}}\n                        </a>\n                    </li>\n                    {{#if settings.returns_enabled}}\n                    <li class=\"header-2__mid-account-list-item\">\n                        <a class=\"header-2__mid-account-list-item-link\" href=\"{{urls.account.returns}}\"\n                            aria-label=\"{{lang 'account.nav.returns'}}\">\n                            {{lang 'account.nav.returns'}}\n                        </a>\n                    </li>\n                    {{/if}}\n                    <li class=\"header-2__mid-account-list-item\">\n                        <a class=\"header-2__mid-account-list-item-link\" href=\"{{urls.account.inbox}}\"\n                            aria-label=\"{{lang 'account.mobile_nav.messages'}}\">\n                            {{lang 'account.mobile_nav.messages'}}\n                        </a>\n                    </li>\n                    <li class=\"header-2__mid-account-list-item\">\n                        <a class=\"header-2__mid-account-list-item-link\" href=\"{{urls.account.addresses}}\"\n                            aria-label=\"{{lang 'account.nav.addresses'}}\">\n                            {{lang 'account.nav.addresses'}}\n                        </a>\n                    </li>\n                    {{#if settings.show_payment_methods}}\n                    <li class=\"header-2__mid-account-list-item\">\n                        <a class=\"header-2__mid-account-list-item-link\" href=\"{{urls.account.payment_methods.all}}\"\n                            aria-label=\"{{lang 'account.nav.payment_methods'}}\">\n                            {{lang 'account.nav.payment_methods'}}\n                        </a>\n                    </li>\n                    {{/if}}\n                    <li class=\"header-2__mid-account-list-item\">\n                        <a class=\"header-2__mid-account-list-item-link\" href=\"{{urls.account.wishlists.all}}\"\n                            aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\">\n                            {{lang 'account.mobile_nav.wishlists'}}\n                        </a>\n                    </li>\n                    <li class=\"header-2__mid-account-list-item\">\n                        <a class=\"header-2__mid-account-list-item-link\" href=\"{{urls.account.recent_items}}\"\n                            aria-label=\"{{lang 'account.nav.recently_viewed'}}\">\n                            {{lang 'account.nav.recently_viewed'}}\n                        </a>\n                    </li>\n                    <li class=\"header-2__mid-account-list-item\">\n                        <a class=\"header-2__mid-account-list-item-link\" href=\"{{urls.account.details}}\"\n                            aria-label=\"{{lang 'account.nav.settings'}}\">\n                            {{lang 'account.nav.settings'}}\n                        </a>\n                    </li>\n                    <li class=\"header-2__mid-account-list-item\">\n                        <a class=\"header-2__mid-account-list-item-link\" href=\"{{urls.auth.logout}}\"\n                            aria-label=\"{{lang 'common.logout'}}\">\n                            {{lang 'common.logout'}}\n                        </a>\n                    </li>\n                </ul>\n            </div>\n            {{else}}\n            {{#if theme_settings.its_popup_login}}\n            <a class=\"navUser-action\" aria-label=\"{{lang 'common.login'}}\" href=\"##\" data-reveal-id=\"popupLoginWindow\">\n                <span class=\"icon icon--account\">\n                    <svg>\n                        <use xlink:href=\"#icon-account\"></use>\n                    </svg>\n                </span>\n                <span class=\"is-srOnly\">\n                    {{lang 'common.login'}}\n                </span>\n            </a>\n            {{else}}\n            <a class=\"navUser-action\" href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'common.login'}}\">\n                <span class=\"icon icon--account\">\n                    <svg>\n                        <use xlink:href=\"#icon-account\"></use>\n                    </svg>\n                </span>\n                <span class=\"navUser--label\">\n                    <span>{{#if customer}}ACCOUNT{{else}}SIGN-IN{{/if}}</span>\n                </span>\n                <span class=\"is-srOnly\">\n                    {{lang 'common.login'}}\n                </span>\n            </a>\n            {{/if}}\n            {{/if}}\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a class=\"navUser-action\" data-cart-preview data-dropdown=\"cart-preview-dropdown\" data-options=\"align:right\"\n                href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\n                <span class=\"icon icon--cart\">\n                    <svg>\n                        <use xlink:href=\"#icon-cart\"></use>\n                    </svg>\n                </span>\n                <span class=\"navUser--label\">\n                    CART\n                </span>\n                <span class=\"navUser-item-cartLabel is-srOnly\">{{lang 'common.cart'}}</span>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n</nav>\n\n\n{{!-- Leftover elements\n{{> components/common/currency-selector}}\n\n{{#if customer.store_credit.value '>' 0}}\n<li class=\"navUser-item\">\n    <a class=\"navUser-action navUser-action--storeCredit\" data-dropdown=\"storeCredit-dropdown\"\n        data-options=\"align:bottom\" href=\"{{urls.cart}}\"\n        aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\">\n        <span class=\"navUser-action-divider\">\n            {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n        </span>\n    </a>\n    <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n        {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n    </div>\n</li>\n{{/if}}\n\n{{#if theme_settings.social_icon_placement_top}}\n<li class=\"navUser-item navUser-item--social\">\n    {{> components/common/social-links}}\n</li>\n<li class=\"navUser-item navUser-item--divider\">|</li>\n{{/if}}\n\n{{#if settings.gift_certificates_enabled}}\n<li class=\"navUser-item\">\n    <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\" aria-label=\"{{lang 'common.gift_cert'}}\">\n        {{lang 'common.gift_cert'}}\n    </a>\n</li>\n{{/if}}\n\n--}}\n","components/custom/header-2/header-2_navigation-list-static":"{{#if (lang 'custom.header.static.navigation-list.shop-all.is-enabled' '===' 'true')}}\r\n<li class=\"navPages-item navPages-item__shop-all\">\r\n    <a class=\"navPages-action has-subMenu navPages-action--toplevel\"\r\n        data-collapsible=\"navigation-item__shop-all\"\r\n        data-collapsible-disabled-breakpoint=\"medium\">\r\n        <span>\r\n            {{lang 'custom.header.static.navigation-list.shop-all.name'}}\r\n        </span>\r\n        <span class=\"icon icon--chevron-down navPages-action-moreIcon\">\r\n            <svg>\r\n                <use xlink:href=\"#icon-chevron-down\"></use>\r\n            </svg>\r\n        </span>\r\n    </a>\r\n    <div class=\"navPage-subMenu\" id=\"navigation-item__shop-all\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <ul class=\"navPage-subMenu-list\">\r\n            {{#for 1 (lang 'custom.header.static.navigation-list.shop-all.number-of-items')}}\r\n            <li \r\n                class=\"navPage-subMenu-item {{#if (lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.has-sub-items')) '===' 'true'}}has-submenu{{/if}}\" \r\n                data-mega-menu-toggle=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\" \r\n                data-collapsible=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\"\r\n                data-collapsible-disabled-breakpoint=\"medium\" \r\n                tabindex=\"-1\" \r\n            >\r\n                {{#if (lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.has-sub-items')) '===' 'true'}}\r\n                <a class=\"navPage-subMenu-action navPages-action has-subMenu\"\r\n                    href=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.url')}}\">\r\n                    <span>\r\n                        {{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.name')}}\r\n                    </span>\r\n                    <span class=\"icon icon--chevron-down\">\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-chevron-down\"></use>\r\n                        </svg>\r\n                    </span>\r\n                </a>\r\n                <a class=\"megaMenu-toggle icon icon--chevron-down\" href=\"##\">\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-chevron-down\"></use>\r\n                    </svg>\r\n                </a>\r\n                {{else}}\r\n                <a class=\"navPage-subMenu-action navPages-action\"\r\n                    href=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.url')}}\">\r\n                    <span>\r\n                        {{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.name')}}\r\n                    </span>\r\n                </a>\r\n                {{/if}}\r\n            </li>\r\n            {{/for}}\r\n        </ul>\r\n        <ul class=\"navPage-megaMenu-list\" style=\"float: right;\">\r\n            {{#for 1 (lang 'custom.header.static.navigation-list.shop-all.number-of-items')}}\r\n                {{#if (lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.is-enabled')) '===' 'true'}}\r\n                <li class=\"navPage-megaMenu\" data-mega-menu-item=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\" id=\"{{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\">\r\n                    <span class=\"navPage-megaMenu__heading\">\r\n                        {{lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.heading')}}\r\n                    </span>\r\n                    <div class=\"navPage-megaMenu__content\">\r\n                        {{#for 1 (lang (concat (concat 'custom.header.static.navigation-list.shop-all.item-' $index)'.mega-menu.number-of-mega-menu-items'))}}\r\n                        <div class=\"navPage-megaMenu__column\">\r\n                            <a class=\"navPage-megaMenu__column-link\"\r\n                                href=\"{{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-' ../$index)'.mega-menu.mega-menu-item-') $index) \".url\")}}\">\r\n                                <img class=\"navPage-megaMenu__image\"\r\n                                    src=\"{{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-' ../$index)'.mega-menu.mega-menu-item-') $index) \".imageUrl\")}}\"\r\n                                    alt=\"{{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-' ../$index)'.mega-menu.mega-menu-item-') $index) \".imageAltText\")}}\">\r\n                                <span>\r\n                                    {{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../$index)'.mega-menu.mega-menu-item-') $index) \".name\")}}\r\n                                </span>\r\n                            </a>\r\n                            {{lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../$index)'.mega-menu.mega-menu-item-') $index) \".name\")}}\r\n                            <ul>\r\n                                {{#for 1 (lang (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../$index)'.mega-menu.mega-menu-item-') $index) \".number-of-sub-items\"))}}\r\n                                    <li class=\"mega-menu-sub-item\">\r\n                                        <a href=\"{{lang (concat (concat (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../../$index)'.mega-menu.mega-menu-item-') ../$index) '.sub-item-') $index) '.url')}}\">{{lang (concat (concat (concat (concat (concat (concat 'custom.header.static.navigation-list.shop-all.item-'../../$index)'.mega-menu.mega-menu-item-') ../$index) '.sub-item-') $index) '.name')}}</a>\r\n                                    </li>\r\n  